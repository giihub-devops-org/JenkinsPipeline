pipeline{
    agent any
    stages{
        stage('Display Hello')
        {
            steps
            {
                sh 'echo Hello Jenkins'

            }
            
        }
        stage('Build stage')
        {
            steps{
                sh "echo Build stage"
            }
        }
        stage('Test stage')
        {
            steps{
                sh "echo Test stage"
            }

        }

    }

}